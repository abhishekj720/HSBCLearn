<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<title>Insert title here</title>
<script>
var json = ("eid" : 1001,
		"ename": "ram",
		"sal" : 34000);

	function display(){
		document.getElementById("teid").innerHTML = json.eid;
		document.getElementById("tname").innerHTML = json.ename;
		document.getElementById("tsal").innerHTML = json.sal;

	}
	
	function clearData(){
		document.getElementById("teid").innerHTML = "";
		document.getElementById("tname").innerHTML = "";
		document.getElementById("tsal").innerHTML = "";
	}



</script>

</head>
<body>
<h1>JSON Demo</h1>
<hr/>
<table cellpadding="10" bgcolor="cyan">
	<tr><td>Name</td>
	<td id="tname"></td>
	</tr>
	<tr><td>salary</td>
	<td id="tsal"></td>
	</tr>
	<tr><td><input type="button" value="populate" onclick="display()"/></td>
	<td><input type="button" value="clear" onclick="clearData()"/></td>
	</tr>
</table>
</body>
</html>